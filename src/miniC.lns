{
   interface IF {
      pub fn func();
   }
   class TestA extend (IF) {
      pub fn func() {
         print( "TestA" );
      }
   }
   class TestB extend (IF) {
      pub fn func() {
         print( "TestB" );
      }
   }

   class Hoge<T> {
      let mut val:T {pub,pub};
   }

   {
      let test:Hoge<&IF> = new Hoge( new TestA() );
      test.$val.func();
   }
   {
      let test:Hoge<&IF> = new Hoge( new TestB() );
      test.$val.func();
   }
}
