form FF( val:int, ... );
fn func( val:int, ... ) {
  print( val, ... );
}
fn func2( ... ) {
   print( ... );
}
form FF3( ...<&stem!> );

fn ret(): int, int, int {
   return 1, 2, 3;
}
fn sub( ff:FF ) {
   ff( ret()** );
   ff( 0, ret()** );
   ff( -1, 0, ret()** );
}
fn sub2( ff:form ) {
   ff( 1 );
   ff( ret()** );
   ff( 0, ret()** );
   ff( -1, 0, ret()** );
}
fn sub3( ff:FF3 ) {
   ff( ret()** );
   ff( 0, ret()** );
   ff( -1, 0, ret()** );
}
sub( func );
sub2( func2 );
sub3( print );
