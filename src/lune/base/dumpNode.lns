fn getTypeListTxt( typeList:&List<&Ast.TypeInfo> ): str {
   let mut txt = "";
   foreach typeInfo, index in typeList {
      if index > 1 {
         txt = txt .. ", ";
      }
      txt = txt .. typeInfo.getTxt( ## );
   }
   return txt;
}

pub override fn dumpFilter.processExpCall(
