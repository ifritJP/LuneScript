   let prefix, depth = opt.get();
   dump( prefix, depth, node, "-> %s" (getTypeListTxt( node.$expTypeList ) ) );
   filter( node.get_func(), self, opt.nextOpt() );
   if! node.get_argList() {
      filter( _exp, self, opt.nextOpt() );
