alge Val {
   val1,
   val2( int, str ),
}

fn sub( val:Val ) {
   match val {
      case .val2( val1, val2 ) {
         print( "val2 %s" (val2) );
      }
      default {
         print( "other" );
      }
   }
   if val == .val1 {
      print( "equal" );
   }
   print( val.$_txt );
}
sub( Val.val1 );
sub( Val.val2( 100, "xyz" ) );
